{
  "redirects": [
    {
      "source": "/referral.html",
      "destination": "/referral",
      "permanent": true
    },
    {
      "source": "/connect.html",
      "destination": "/connect",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/referral",
      "destination": "/referral.html"
    },
    {
      "source": "/connect",
      "destination": "/connect.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

